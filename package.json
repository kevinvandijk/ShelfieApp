{
  "name": "ShelfieApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "NODE_ENV=test node_modules/.bin/ava",
    "test:watch": "NODE_ENV=test node_modules/.bin/ava --watch",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc ava",
    "tron": "node_modules/.bin/reactotron",
    "ios": "react-native run-ios",
    "clear:cache": "rm -rf node_modules && watchman watch-del-all && npm install && npm start --reset-cache"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "tests/**/*.js",
      "!tests/setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./tests/setup.js"
    ]
  },
  "nyc": {
    "include": [
      "app/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "apisauce": "^0.3.0",
    "react": "15.2.1",
    "react-native": "0.31.0",
    "react-native-device-info": "^0.9.3",
    "react-native-fabric": "^0.3.0",
    "react-native-i18n": "0.0.8",
    "react-native-router-flux": "^3.34.0",
    "react-native-svg": "^4.1.1",
    "react-native-vector-icons": "^2.0.3",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.0",
    "redux-storage": "^4.1.0",
    "redux-storage-engine-reactnativeasyncstorage": "^1.0.1",
    "reduxsauce": "^0.1.0",
    "seamless-immutable": "^6.1.1",
    "stacktrace-js": "^1.3.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^1.0.3",
    "enzyme": "^2.4.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "mockery": "^1.7.0",
    "nyc": "^7.1.0",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "reactotron": "^0.9.0"
  }
}
