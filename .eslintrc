{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
      "react"
  ],
  "rules": {
    "comma-dangle": 0,
    "arrow-body-style": [2, "as-needed", { "requireReturnForObjectLiteral": true }],
    "react/jsx-curly-spacing": [2, "always", {"spacing": {
        "objectLiterals": "never"
    }}]
  },
  "globals": {
    "__DEV__": true
  }
}
