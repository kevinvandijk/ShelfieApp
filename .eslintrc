{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
      "react"
  ],
  "rules": {
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "arrow-parens": [2, "always"],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-underscore-dangle": 0,
    "react/jsx-curly-spacing": [2, "always", {
      "spacing": {
        "objectLiterals": "never"
      },
      "allowMultiline": true
    }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "global-require": 0
  },
  "globals": {
    "__DEV__": true,
    "jest": true,
    "describe": true,
    "it": true,
    "xit": true,
    "expect": true
  }
}
